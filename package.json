{
  "name": "dboydgit-portfolio",
  "version": "0.0.1",
  "description": "Portfolio site for Derryk Boyd, @dboydgit on GitHub",
  "keywords": [
    "developer",
    "portfolio"
  ],
  "license": "MIT",
  "author": "Derryk Boyd",
  "scripts": {
    "copy": "copyfiles robots.txt humans.txt favicon.ico dist",
    "build": "parcel build *.html --public-url /portfolio/ && npm run copy",
    "dev": "parcel index.html --open --public-url /portfolio/",
    "start": "npm run build && npm run dev",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "main": "index.html",
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dboydgit/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/dboydgit/portfolio/issues"
  },
  "homepage": "http://dboydgit.github.io/portfolio/"
}
